<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PokemonController.HomeScreenController">
   <children>
      <Button fx:id="btnStartGame" layoutX="229.0" layoutY="279.0" mnemonicParsing="false" onAction="#btnStartGame" text="START">
         <font>
            <Font size="25.0" />
         </font>
      </Button>
      <Label fx:id="lblStartText" layoutX="188.0" layoutY="170.0" text="Start your journey!">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <ImageView fx:id="imgPokeballs" fitHeight="137.0" fitWidth="180.0" layoutX="188.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../ImagesAndSprites/Pokeballs.jpeg" />
         </image>
      </ImageView>
   </children>
</AnchorPane>

